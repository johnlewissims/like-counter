{
  "name": "ejin/like-counter",
  "description": "A simple plugin that displays total user likes on the user profile.",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.8"
  },
  "authors": [
    {
      "name": "John Lewis Sims",
      "email": "john@ejincollective.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ejin\\LikeCounter\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Like Counter",
      "icon": {
        "name": "fas fa-plus",
        "backgroundColor": "#2D2927",
        "color": "#76B9DB"
      }
    },
    "flagrow": {
      "discuss": ""
    }
  }
}
